       <br />\r\n                            <p class=\"contenu_message margeG \"><span>Prenom:</span>{{user.Prenom}}</p>\r\n                            <br />\r\n                            <p class=\"contenu_message margeG \"><span>Email:</span>{{user.Email}}</p>\r\n                            <br />\r\n                            <p class=\"contenu_message font \">{{message.createdAt}}</p> \r\n                        </div>\r\n                        \r\n                    </div>\r\n                </li >\r\n            </ul>\r\n    </div>\r\n    <Footer />\r\n</template> \r\n<script>\r\nimport Header from '@/components/Header.vue';\r\nimport Footer from '@/components/Footer.vue';\r\nimport axios from \"axios\";\r\nexport default {\r\n    components : {\r\n        Header,\r\n        Footer\r\n    },\r\n    name : 'DetailsMessage',\r\n    data () {\r\n        return {\r\n            user: {\r\n                nom: \"\",\r\n                prenom: \"\",\r\n                Email: \"\",\r\n            },\r\n        }  \r\n    },\r\n    // Affichage d 'un message\r\n\r\n    mounted ()  {\r\n    ///////////////////////    console.log(this.$route.params);\r\n        this.AppelUser(this.$route.params.id);\r\n    },\r\n    methods: {\r\n\r\n        AppelUser(userId){\r\n            const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n        axios.get(\"http://localhost:3000/api/users/display/\" + userId , config )\r\n           .then( (res) => {\r\n              const data = res.data;\r\n             this.user = data;\r\n             console.log(data);\r\n           });\r\n        },\r\n         /////////////// Modification du message   \r\n      \r\n        modifyUser(e) {\r\n            e.preventDefault();\r\n            const userId = this.user.id\r\n            const bodyFormData = new FormData();\r\n            bodyFormData.append('nom', this.nom);\r\n            bodyFormData.append('prenom', this.prenom);\r\n            bodyFormData.append('email', this.Email);\r\n\r\n            console.log(this.nom, this.prenom, this.Email);\r\n\r\n            const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n            \r\n        axios.put(\"http://localhost:3000/api/users/modify/\" + userIdId, config )\r\n            .then(res => {\r\n                    const data = res.data;\r\n                    this.user = data;\r\n            });\r\n        }\r\n  }\r\n         \r\n  }\r\n</script>\r\n\r\n<style>\r\n.list {\r\n     list-style-type: none;\r\n}\r\n\r\n#message_titre .contenu_message {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#affichage_message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300px;\r\n\r\n  display: inline-block;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\n\r\np {\r\n  font-size: 1em;\r\n  text-align: center;\r\n}\r\n#valider_message {\r\n  background:  #F4511E;           \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color:  #F4511E;\r\n  border: none;\r\n}\r\n.titre .contenu #image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n.affichage_image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n#container{\r\n    align-items: center;\r\n}\r\n#formulaire {\r\n  align-items: center;\r\n}\r\n.btn {\r\n  border: 0;\r\n  line-height: 2.5;\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  font-size: 1em;\r\n  text-align: center;\r\n  border-radius: 10px;\r\n  background-color:  #F4511E;\r\n}\r\n#boutons {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n}\r\n.font {\r\n    font-weight: bold;\r\n    font-size: 0.8em;\r\n}\r\nspan {\r\n    text-decoration