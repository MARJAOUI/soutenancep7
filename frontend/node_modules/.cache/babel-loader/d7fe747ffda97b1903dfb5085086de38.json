I,EAAC,SAAkC;;aAAf,KAAA,CAAA,IAAA,CAAK,OAAL,GAAY,M;;AAAG,GAA1F,E,IAAA,E;;AAAA,G,iBAA2E,KAAA,CAAA,IAAA,CAAK,O,GAIlF,EAHE,WAGF,EAHQ,WAGR,EADE,YAAA,CAA2G,OAA3G,EAA2G;AAAnG,IAAA,OAAK,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,MAAA,CAAA,CAAA,CAAA,GAAA;AAAA,aAAE,QAAA,CAAA,YAAA,IAAA,QAAA,CAAA,YAAA,OAAA,QAAA,YAAF;AAAA,KAAA,CAA8F;AAA9E,IAAA,EAAE,EAAC,gBAA2E;AAAzD,IAAA,IAAI,EAAC,QAAoD;AAA3C,IAAA,KAAK,EAAC,cAAqC;AAApB,IAAA,KAAK,EAAC;AAAc,GAA3G,CACF,CAtBN,CAuBF,CA1BN,C,EA2BA,YAAA,CAAU,iBAAV,C","sourcesContent":["<template>\r\n<Header />\r\n\r\n    <div class=\"container forms\">\r\n     <Alert  v-if=\"alert\" v-bind:message=\"alert \" />   \r\n        <h1 class=\"title is-size-1\">Modifier et Valider </h1>\r\n            <div id=\"formulaire\">\r\n                    <label class=\"form_col\" for=\"contact_nom\">Nom </label>\r\n                    <input name=\"contact_nom\" id=\"contact_nom\" type=\"text\" v-model=\"user.nom\"  > \r\n                    <br /><br />\r\n\r\n                    <label class=\"form_col\" for=\"contact_prenom\">Prénom </label>\r\n                    <input name=\"contact_prenom\" id=\"contact_prenom\" type=\"text\" v-model=\"user.prenom\"  >\r\n                    <br /><br />\r\n                    \r\n                    <label class=\"form_col\" for=\"contact_email\">Email </label>\r\n                    <input name=\"contact_email\" id=\"contact_email\"  type=\"Email\" v-model=\"user.email\" >\r\n                    <br /><br />\r\n\r\n                    <label class=\"form_col\" for=\"contact_password\">Password </label>\r\n                    <input name=\"contact_password\" id=\"contact_password\"  type=\"Password\" v-model=\"user.password\" >\r\n                    <br /><br />\r\n\r\n                    <label class=\"form_col\" for=\"contact_isAdmin\">Admin </label>\r\n                    <input name=\"contact_Admin\" id=\"contact_password\"  type=\"boolean\" v-model=\"user.isAdmin\" >\r\n                    <br /><br />\r\n                    \r\n                    <input @click=\"modification\" id=\"valider_signup\"  type=\"submit\" class=\"btn_4 styled\"   value=\"Soumettre\" />\r\n            </div>\r\n    </div>\r\n    <Footer />\r\n</template>\r\n\r\n<script>\r\nimport Header from '@/components/Header.vue';\r\nimport Footer from '@/components/Footer.vue';\r\nimport Alert from '@/components/Alert.vue';\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n    name: \"ModifyUser\",\r\n    components: {\r\n        Header, Footer, Alert\r\n    },\r\n    data() {\r\n        return {\r\n            user : {nom: null , prenom: null, email: null, password: null, isAdmin: null},\r\n            alert: \"\",\r\n        }\r\n    },\r\n    methods :{\r\n        modification (userId) {\r\n        const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n    axios.put('http://localhost:3000/api/users/modify/' + userId, {\r\n        nom       : this.user.nom,\r\n        prenom    : this.user.prenom,\r\n        email     : this.user.email,\r\n        password  : this.user.password,\r\n        isAdmin   : this.user.isAdmin\r\n      },\r\n      config\r\n      )\r\n      .then(() => {\r\n          this.$router.push({ path: \"/\", query: {alert:'user modifié avec succés!'} });\r\n        })  \r\n    .catch(err => console.error(err));\r\n  }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.forms {\r\n    margin-top: 160px;\r\n    margin-bottom: 175px;\r\n}\r\n.title {\r\n    color: #122442;\r\n}\r\n.form_col {\r\n    display: inline-block;\r\n    margin-right: 15px;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\n#valider_signup{\r\n\tbackground: #f57a54;          \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n}\r\ninput {\r\n\tpadding: 2px;\r\n\tborder: 1px solid #CCC;\r\n\tborder-radius: 2px;\r\n\toutline: none; \r\n}\r\n#formulaire{\r\n    align-items: center;\r\n}\r\n/*.alerts {\r\n    height : 20px;\r\n    width: 500px;\r\n    border: 2px, solid, red;\r\n}*/\r\n\r\n</style>"],"sourceRoot":""}]}