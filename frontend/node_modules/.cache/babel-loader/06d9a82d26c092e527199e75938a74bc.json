e margeG \"><span>Message :</span>{{message.contenu}}</p>\r\n                        </div>\r\n                        <div id=\"align_nom_date\">   \r\n                            <p class=\"margeD\">{{message.User.nom}}  {{ message.User.prenom}}</p>  \r\n                            <p class=\"margeD\">{{message.createdAt}}</p>                        \r\n                        </div> \r\n                    </div>\r\n                     <div id=\"boutons\">\r\n                      <router-link class=\"btn btn-default\" v-bind:to=\"'/DetailsMessage/' +message.id\">Détail</router-link> \r\n                     </div>\r\n                   \r\n                </li >\r\n                 <div> <hr> </div>  \r\n            </ul>\r\n        </div>\r\n    </div>\r\n<Footer />  \r\n</template>\r\n<script>\r\nimport Header2 from '@/components/Header2.vue'\r\nimport Footer from '@/components/Footer.vue'\r\nimport axios from 'axios';\r\nexport default {\r\n    name: 'DisplayMessages',\r\n    components: {\r\n      Header2, Footer\r\n  },\r\n    data() { \r\n        return { \r\n            messages : \"\",\r\n  }\r\n  },\r\n    mounted(){  \r\n             const config = {\r\n            headers: { Authorization: \"Bearer \" + localStorage.token } \r\n            }\r\n        \r\n    axios.get(\"http://localhost:3000/api/messages/display\", config )\r\n        .then(res => {\r\n                const data = res.data;\r\n                this.messages = data;\r\n            });\r\n   },\r\n   methods :{\r\n        DisplayThisMessage: function (id) {\r\n            const config = {\r\n            headers: { Authorization: \"Bearer \" + localStorage.token } \r\n            }\r\n            axios.get('http://localhost:3000/api/messages/display/', {\r\n                messageId: id,\r\n                config\r\n            })\r\n                .then(() => {\r\n                   // console.log();\r\n                    this.$router.push({ path: \"/DisplayOneMessage\" });\r\n                })  \r\n                .catch(err => console.error(err));\r\n        }\r\n    }\r\n}\r\n</script>\r\n<style>\r\n#message {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n#message_accueil {\r\n    color: #F4511E;\r\n    font-size: 1.5em;\r\n}\r\n.list {\r\n    list-style-type: none;\r\n}\r\n.list_content {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-around;\r\n}\r\n #align_nom_date #align_photo {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    width: max-content;\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\nspan {\r\n    color: red;\r\n}\r\n #align_nom_date {\r\n     padding-bottom: 0;\r\n\r\n }\r\n.margeD {\r\n    margin:  0px;\r\n    font-size: 0.6em;\r\n    font-weight: bold;\r\n}\r\n.margeG {\r\n    text-align: left;\r\n    margin: 0px;  \r\n}\r\n#global {\r\n    max-width: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n}\r\n.contenu_message {\r\n    width: 300px;\r\n    height: auto; \r\n}\r\n.bouton {\r\n  border: 0;\r\n  line-height: 2.5;\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  font-size: 1em;\r\n  text-align: center;\r\n  border-radius: 10px;\r\n  background-color: #F4511E;\r\n}\r\n#boutons {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-around;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}