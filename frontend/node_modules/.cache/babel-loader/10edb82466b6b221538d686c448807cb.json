 headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n      axios.post(\"http://localhost:3000/api/messages/create\",  {\r\n        titre   : this.message.titre,\r\n        contenu : this.message.contenu,\r\n        UserId  : this.message.UserId,\r\n        },\r\n         config\r\n        )\r\n        .then(response => {\r\n            this.titre   = response.titre;\r\n            this.contenu = response.contenu;\r\n            this.UserId  = response.UserId;\r\n      }) \r\n        .catch((err) => {\r\n          console.log(err);\r\n        });\r\n    },\r\n  /*methods: {  \r\n    afficherMessages(){\r\n      const config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n    axios.get(\"http://localhost:3000/api/messages/display\", {\r\n     config\r\n    }).then(res => (this.articles = res.data)\r\n    )\r\n  },\r\n }*/\r\n  }\r\n    \r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#message_titre #message_contenu {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300px;\r\n\r\n  display: inline-block;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\nbutton {\r\n  width: 170px;\r\n  margin-top: 30px;\r\n  background: #F4511E;\r\n   margin-left: 300px;\r\n}\r\np {\r\n  font-size: 1.5em;\r\n  color: #F4511E;\r\n  text-align: center;\r\n  text-decoration: underline;\r\n}\r\n#valider_message {\r\n  background: #F4511E;           \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\n</style>"],"sou