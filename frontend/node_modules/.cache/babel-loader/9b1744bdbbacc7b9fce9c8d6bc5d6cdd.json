el=\"message.titre\">\r\n      <br /><br />\r\n      <label id='message_contenu' for=\"contenu\">Message:</label>\r\n      <textarea  name=\"contenu\" v-model=\"message.contenu\">Vous pouvez écrire votre commentaire ici</textarea><br/>\r\n      <br /><br />\r\n    </div> \r\n\r\n    <div class=\"image\">\r\n      <label for=\"formFileSm\"  class=\"form-label d-flex align-items-start\" >Ajoutez une image</label>\r\n      <br /><br />\r\n      <input id=\"message_image\"    type=\"file\" name=\"image\" ref=\"fileInput\" @change=\"onFileUpload\" >\r\n    </div>\r\n    </form>\r\n    <!--<input type=\"hidden\" name=\"messageId\" :value=UserId>  -->\r\n    <button type=\"submit\" @click=\"createMessage\">Soumettre</button> \r\n    \r\n  </div>\r\n     \r\n  \r\n  <div  class=\"affichage_messages\">\r\n    <p>Messages reçus</p>\r\n  </div> \r\n  <Footer />   \r\n</template>\r\n<script>\r\nimport Header from '@/components/Header.vue'\r\nimport Footer from '@/components/Footer.vue'\r\nimport axios from \"axios\";\r\nexport default {                                 //   1\r\n  name: \"Application\",\r\n  components : {\r\n    Header, Footer\r\n  },\r\n  data() { \r\n     return {\r\n          message: { titre: null, contenu: null, imageUrl: null}    // test 0704 20h09\r\n        };\r\n  },\r\n  methods: {\r\n    onFileUpload(evenement) {\r\n      this.selectedFile = evenement.target.files[0];\r\n    },\r\n    createMessage(message) {\r\n     /* const config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n        const formData = new FormData()\r\n      formData.append('image', this.selectedFile);\r\n      formData.append('message',message);*/\r\n      var bodyFormData = new FormData();\r\n      bodyFormData.append('image', this.selectedFile);\r\n      bodyFormData.append('message',message);\r\n      \r\n      axios({\r\n          method: \"post\",\r\n          url: \"http://localhost:3000/api/messages/create\",\r\n          data: bodyFormData,\r\n          headers: { \"Content-Type\": \"multipart/form-data\" , Authorization : 'token'  },\r\n        })\r\n          .then(function (response) {\r\n            //handle success\r\n            console.log(response);\r\n          })\r\n          .catch(function (response) {\r\n            //handle error\r\n            console.log(response);\r\n          });\r\n      \r\n     /* axios.post(\"http://localhost:3000/api/messages/create\", data: bodyFormData, config)   \r\n        .then(() => {\r\n             alert('Votre post à bien été enregistré !');\r\n        }) \r\n        .catch((err) => {\r\n          console.log(err);\r\n        });*/\r\n    }\r\n \r\n  }\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n#message_titre #message_contenu {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300px;\r\n\r\n  display: inline-block;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\nbutton {\r\n  width: 170px;\r\n  margin-top: 30px;\r\n  background: #F4511E;\r\n   margin-left: 300px;\r\n}\r\np {\r\n  font-size: 1.5em;\r\n  color: #F4511E;\r\n  text-align: center;\r\n  text-decoration: underline;\r\n}\r\n#valider_message {\r\n  background: #F4511E;           \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\n.titre {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n.contenu {\r\n  height: 100px;\r\n  width: 200px;\r\n}\r\n.image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n</style>"],"sourceRoot":""}]}