config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        }\r\n    axios.get(\"http://localhost:3000/api/messages/display\", config )\r\n        .then(res => {\r\n                const data = res.data;\r\n                this.messages = data;\r\n                const affichage_image = document.querySelector('#message_image');\r\n                const file = this.message.imageUrl; \r\n                affichage_image.append('afterbegin', file);\r\n            });\r\n   },\r\n   methods :{\r\n        suppression (req) {\r\n        const config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n        const id = (req.params.id);\r\n    axios.post('http://localhost:3000/api/messages/delete/:id' + id , config  )\r\n      .then(() => {\r\n          this.$router.push({ path: \"/AffichageMessages\" });\r\n        })  \r\n    .catch(err => console.error(err));\r\n  }\r\n    }\r\n }  \r\n</script>\r\n\r\n<style>\r\n#message {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n#message_accueil {\r\n    color: #F4511E;\r\n    font-size: 1.5em;\r\n}\r\n.list {\r\n    list-style-type: none;\r\n}\r\n.list_content {\r\n    display: flex;\r\n    flex-direction: row;\r\n    justify-content: space-around;\r\n}\r\n #align_nom_date #align_photo {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    width: max-content;\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\nspan {\r\n    color: red;\r\n}\r\n #align_nom_date {\r\n     padding-bottom: 0;\r\n\r\n }\r\n.margeD {\r\n    margin:  0px;\r\n    font-size: 0.6em;\r\n    font-weight: bold;\r\n}\r\n.margeG {\r\n    text-align: left;\r\n    margin: 0px;  \r\n}\r\n#global {\r\n    max-width: auto;\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-around;\r\n}\r\n.contenu_message {\r\n    width: 300px;\r\n    height: auto; \r\n}\r\n.bouton {\r\n  border: 0;\r\n  line-height: 2.5;\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  font-size: 1em;\r\n  text-align: center;\r\n  border-radius: 10px;\r\n  background-color: #F4511E;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}