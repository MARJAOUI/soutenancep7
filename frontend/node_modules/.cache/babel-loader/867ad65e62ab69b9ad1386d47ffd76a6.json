\n    </div>\r\n    <div class=\"contenu\">\r\n      <label id='message_contenu' for=\"contenu\">Message:</label>\r\n      <textarea v-model=\"contenu\" name=\"contenu\">Vous pouvez écrire votre commentaire ici</textarea><br/>\r\n      <br /><br />\r\n    </div> \r\n    <div class=\"image\">\r\n      <label for=\"formFileSm\"  class=\"form-label d-flex align-items-start\">Ajoutez une image</label>\r\n      <br /><br />\r\n      <input id=\"message_image\"    type=\"file\" name=\"image\" ref=\"fileInput\" @change=\"onFileUpload\">\r\n      \r\n    </div>\r\n\r\n\r\n\r\n      <!--<label id='message_image' for=\"contenu\">Image:</label>\r\n      <textarea v-model=\"imageUrl\" name=\"Image\">Choisissez une animation</textarea><br/>  -->\r\n\r\n      <!--<label for=\"formFileSm\"  class=\"form-label d-flex align-items-start\">Ajoutez une image</label>\r\n      <input classe=\"form-control form-control-sm\"  type=\"file\"  id=\"file\"  ref=\"fileInput\"  v-on:change=\"selectedFile\" > < onFileUpload\r\n       -->\r\n\r\n    \r\n      \r\n\r\n      <input type=\"hidden\" name=\"messageId\" :value=UserId>\r\n      \r\n    <button type=\"submit\" @click=\"createMessage\">Soumettre</button> \r\n    \r\n  </div>\r\n     \r\n  \r\n <!-- <div><hr></div>\r\n    <div  class=\"affichage_messages\">\r\n      <p>Messages reçus</p>\r\n      <div id=\"message\">\r\n        <input type=\"text\" v-model=\"titre\" name=\"titre\" id=\"titres\">\r\n        <input type=\"text\" v-model=\"contenu\" name=\"contenu\" id=\"contenus\">\r\n        <input type=\"image\"  v-model=\"image\" name=\"image\"  id=\"images\">\r\n      <div> \r\n  </div> -->\r\n     \r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {                                 //   1\r\n  name: \"Application\",\r\n  data() { \r\n    \r\n     return {\r\n          message: { titre: null, contenu: null, imageUrl: null },\r\n          selectedFile: null\r\n        };\r\n  },\r\n  methods: {\r\n    onFileUpload(evenement) {\r\n      this.selectedFile = evenement.target.files[0];\r\n      console.log(this.selectedFile);\r\n    },\r\n        \r\n    createMessage() {\r\n      const config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n        let fd = new FormData();\r\n                fd.append('titre', this.titre);\r\n                fd.append('contenu', this.contenu);\r\n                fd.append('image', this.selectedFile, this.selectedFile.name);\r\n      axios.post(\"http://localhost:3000/api/messages/create\", fd, config)    \r\n        .then(() => {\r\n             alert('Votre post à bien été enregistré !');\r\n        }) \r\n        .catch((err) => {\r\n          alert('votre message ne peut etre enregistré !')\r\n          console.log(err);\r\n        });\r\n    }\r\n /* methods: {  \r\n    afficherMessages(){\r\n      const config = {\r\n            headers: {\r\n                'Content-Type': 'application/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n    axios.get(\"http://localhost:3000/api/messages/display\", {\r\n     config\r\n    }).then(res => (this.messages = res.data)\r\n    )\r\n  },\r\n }*/\r\n  }\r\n    \r\n  \r\n  }\r\n  \r\n  \r\n  \r\n\r\n    \r\n\r\n</script>\r\n\r\n<style scoped>\r\n#message_titre #message_contenu {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300px;\r\n\r\n  display: inline-block;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\nbutton {\r\n  width: 170px;\r\n  margin-top: 30px;\r\n  background: #F4511E;\r\n   margin-left: 300px;\r\n}\r\np {\r\n  font-size: 1.5em;\r\n  color: #F4511E;\r\n  text-align: center;\r\n  text-decoration: underline;\r\n}\r\n#valider_message {\r\n  background: #F4511E;           \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\n.titre {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n.contenu {\r\n  height: 100px;\r\n  width: 200px;\r\n}\r\n.image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n</style>"],"sourceRoot":""}]}