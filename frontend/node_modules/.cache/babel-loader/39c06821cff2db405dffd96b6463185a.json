        const data = res.data;\r\n                this.messages = data;\r\n                const affichage_image = document.querySelector('#message_image');\r\n                const file = this.message.imageUrl; \r\n                affichage_image.append('afterbegin', file);\r\n               \r\n            })\r\n              //// .then((response) => (this.messages = response));\r\n  }\r\n }  \r\n\r\n</script>\r\n\r\n<style>\r\n#message {\r\n    display: flex;\r\n    flex-direction: column;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n}\r\n#message_accueil {\r\n    color: #F4511E;\r\n    font-size: 1.5em;\r\n}\r\n.list {\r\n    list-style-type: none;\r\n}\r\n\r\n</style>"],"sourceRoot":""}]}