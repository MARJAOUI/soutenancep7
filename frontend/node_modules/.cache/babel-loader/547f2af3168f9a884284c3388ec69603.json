"formulaire\">\r\n\r\n            <!--  <label id='message_image' for=\"imageUrl\">Image:</label>\r\n              <img :src=\"message.imageUrl\" id=\"message_image\" alt=\" \"/>\r\n              <br /><br />  -->\r\n\r\n              <label id='message_titre' for=\"titre\">Titre:</label>\r\n              <input type=\"text\"  name=\"titre\" id=\"titre\" v-model=\"titre\">\r\n              <br /><br />\r\n\r\n              <label id='message_contenu' for=\"contenu\">Texte:</label>\r\n              <textarea  name=\"contenu\" v-model=\"contenu\">Texte:</textarea><br/>\r\n              <br /><br />\r\n\r\n              \r\n              <label for=\"File\"  class=\"form-label d-flex align-items-start\" >Modifier l'image image:</label>\r\n              <input id=\"image\"    type=\"file\" name=\"image\" ref=\"fileInput\" @change=\"onFileUpload\" >\r\n              <br /><br />\r\n\r\n              <button type=\"submit\"  >Soumettre</button>  \r\n             <!-- <input @submit.prevent=\"submitComment\" id=\"valider_message\"  type=\"submit\" class=\"btn_4 styled\"   value=\"Valider\" />  -->\r\n            </div>\r\n      </div>\r\n    </form> \r\n\r\n    <br />\r\n    <br />\r\n  <Footer />\r\n</template>\r\n\r\n<script>\r\nimport Header3 from '@/components/Header3.vue'\r\nimport Footer from '@/components/Footer.vue'\r\nimport axios from \"axios\";\r\nexport default {\r\n    data() {\r\n      return {\r\n          message: \"\",\r\n      }  \r\n    },\r\n    components: {\r\n        Header3, Footer\r\n    },\r\n    methods: {\r\n     onFileUpload(evenement) {\r\n             this.selectedFile = evenement.target.files[0];\r\n    },\r\n\r\n     modifyMessage(e) {\r\n            e.preventDefault();\r\n            const messageId = this.message.id\r\n            const bodyFormData = new FormData();\r\n            bodyFormData.append('contenu', this.contenu);\r\n            bodyFormData.append('titre', this.titre);\r\n            bodyFormData.append('image', this.selectedFile);\r\n            console.log(messageId);\r\n          //  console.log(this.titre, this.contenu, this.selectedFile.name);\r\n\r\n            const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n            \r\n        axios.put(\"http://localhost:3000/api/messages/modify/\" + messageId, config )\r\n            .then(res => {\r\n                    const data = res.data;\r\n                    this.message = data;\r\n            });\r\n        },    \r\n\r\n       /* onFileUpload(evenement) {\r\n             this.selectedFile = evenement.target.files[0];\r\n    },\r\n        AjoutMessage(e) {\r\n            e.preventDefault();\r\n            const messageId = this.message.id\r\n            const bodyFormData = new FormData();\r\n            bodyFormData.append('contenu', this.contenu);\r\n            bodyFormData.append('titre', this.titre);\r\n            bodyFormData.append('image', this.selectedFile);\r\n\r\n            console.log(this.titre, this.contenu, this.selectedFile.name);\r\n        \r\n            axios({\r\n          method: \"post\",\r\n          url: \"http://localhost:3000/api/messages/create\", \r\n          //  récupération autorisation\r\n           headers: { Authorization: \"Bearer \" + localStorage.token },\r\n          data: bodyFormData,\r\n        })\r\n       \r\n          .then((response) =>  {\r\n              this.$router.push({ path: \"/DisplayMessages\" });\r\n            //handle success\r\n            console.log(response);\r\n          })\r\n          .catch(function (response) {\r\n            //handle error\r\n            console.log(response);\r\n          });\r\n\r\n        },*/\r\n    },\r\n  }\r\n</script>\r\n\r\n<style scoped>\r\n#message_titre #message_contenu {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#affichage_message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300px;\r\n\r\n  display: inline-block;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\n\r\np {\r\n  font-size: 1.5em;\r\n  color: #F4511E;\r\n  text-align: center;\r\n  text-decoration: underline;\r\n}\r\n#valider_message {\r\n  background: #F4511E;           \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n}\r\nhr {\r\n  height: 3px;\r\n  background-color: #F4511E;\r\n  border: none;\r\n}\r\n.titre .contenu #image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n.affichage_image {\r\n  height: 50px;\r\n  width: 200px;\r\n}\r\n#container{\r\n    align-items: center;\r\n}\r\n#formulaire {\r\n  align-items: center;\r\n}\r\nbutton {\r\n  border: 0;\r\n  line-height: 2.5;\r\n  padding: 0 20px;\r\n  margin-top: 20px;\r\n  font-size: 1em;\r\n  text-align: center;\r\n  border-radius: 10px;\r\n  background-color: #F4511E;\r\n}\r\n</style>"],"sourceRoot":""}]}