nom: \"\",\r\n                prenom: \"\",\r\n                Email: \"\",\r\n            },\r\n        }  \r\n    },\r\n    // Affichage d 'un message\r\n\r\n    mounted ()  {\r\n    ///////////////////////    console.log(this.$route.params);\r\n        this.AppelUser(this.$route.params.id);\r\n    },\r\n    methods: {\r\n\r\n        AppelUser(userId){\r\n            const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n        axios.get(\"http://localhost:3000/api/users/display/\" + userId , config )\r\n           .then( (res) => {\r\n              const data = res.data;\r\n             this.user = data;\r\n             console.log(data);\r\n           });\r\n        },\r\n         /////////////// Modification du message   \r\n      \r\n        modifyUser(e) {\r\n            e.preventDefault();\r\n            const userId = this.user.id\r\n            const bodyFormData = new FormData();\r\n            bodyFormData.append('nom', this.nom);\r\n            bodyFormData.append('prenom', this.prenom);\r\n            bodyFormData.append('email', this.Email);\r\n\r\n            console.log(this.nom, this.prenom, this.Email);\r\n\r\n            const config = {\r\n                headers: { Authorization: \"Bearer \" + localStorage.token } \r\n                }\r\n            \r\n        axios.put(\"http://localhost:3000/api/users/modify/\" + userIdId, config )\r\n            .then(res => {\r\n                    const data = res.data;\r\n                    this.user = data;\r\n            });\r\n        }\r\n  }\r\n         \r\n  }\r\n</script>\r\n\r\n<style>\r\n.list {\r\n     list-style-type: none;\r\n}\r\n\r\n#message_titre .contenu_message {\r\ntext-align: left;\r\n}\r\n#container {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n}\r\n#affichage_message {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: space-around;\r\n  margin-left: 300