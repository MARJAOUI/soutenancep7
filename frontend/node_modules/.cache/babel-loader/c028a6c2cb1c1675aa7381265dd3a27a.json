cation/json',\r\n                Authorization : 'token'\r\n            }\r\n        };\r\n    axios.post('http://localhost:3000/api/users/signup', {\r\n        nom       : this.user.nom,\r\n        prenom    : this.user.prenom,\r\n        email     : this.user.email,\r\n        password  : this.user.password\r\n      },\r\n      config\r\n      )\r\n      .then(() => {\r\n          this.$router.push({ path: \"/signin\" });\r\n        })  \r\n      /*.then(response => {\r\n            this.nom      = response.nom;\r\n            this.prenom   = response.prenom;\r\n            this.email    = response.email;\r\n            this.password = response.password;\r\n      }) */\r\n    .catch(err => console.error(err));// .catch(() => this.errored = true)//.catch(errors => {\r\n  }\r\n    }\r\n    \r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.forms {\r\n    margin-top: 160px;\r\n    margin-bottom: 175px;\r\n}\r\n.title {\r\n    color: #122442;\r\n}\r\n.form_col {\r\n    display: inline-block;\r\n    margin-right: 15px;\r\n    padding: 3px 0px;\r\n    width: 100px;\r\n    min-height: 1px;\r\n    text-align: left;\r\n}\r\n#valider_signup{\r\n\r\n\tbackground: #F4511E;          \r\n\tcolor: black;\r\n    width: 170px;\r\n\tmargin-left: 120px;\r\n\tmargin-bottom: 100px;\r\n}\r\ninput {\r\n\tpadding: 2px;\r\n\tborder: 1px solid #CCC;\r\n\tborder-radius: 2px;\r\n\toutline: none; \r\n}\r\n#formulaire{\r\n    align-items: center;\r\n}\r\n</style>"],"sourceRoot":""}]}